 #include<stdio.h>
#include<string.h>
int f(char);
int g(char);
void infixtopostfix(char in[],char post[])
{
int i=0,j=0,top=-1;
char stk[20],sym;
stk[++ top]='#';
while(in[i]!='\0')
{
sym=in[i++];
while(f(stk[top])>g(sym))
post[j++]=stk[top--];
if(f(stk[top])!=g(sym))
stk[++top]=sym;
else
stk[top--];
}
while(stk[top]!='#')

